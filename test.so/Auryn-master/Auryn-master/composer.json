    "require-dev": {
        "phpunit/phpunit": "^4.4.5"
    },
    "require": {
        "php": ">=5.3.0"
    },
    "require-dev": {
        "phpunit/phpunit": "~4.5",
        "athletic/athletic": "~0.1"
    },
    "autoload": {
        "psr-4": {
            "Auryn\\": "lib/"
        }
